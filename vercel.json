{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/browser",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/apps/api/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "apps/api/api/index.ts": {
            "runtime": "nodejs20.x",
            "memory": 1024,
            "maxDuration": 10
        }
    }
}